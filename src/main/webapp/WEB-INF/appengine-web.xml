<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>ekikara2oudia</application>
  <version>2</version>
  <sessions-enabled>true</sessions-enabled>
  <ssl-enabled>true</ssl-enabled>
  <precompilation-enabled>true</precompilation-enabled>
  <static-files>
    <include path="/css/**.css"/>
    <include path="/js/**.js"/>
    <include path="/img/**.png"/>
    <include path="/img/**.gif"/>
    <include path="/img/**.jpg"/>
    <include path="/error/**.html"/>
    <include path="/*.bz2"/>
    <include path="/favicon.ico"/>
  </static-files>
  <threadsafe>true</threadsafe>
  <system-properties>
    <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
<!--     <property name="java.util.logging.config.file" value="WEB-INF/classes/log4j.properties"/> -->
    <property name="file.encoding" value="MS932"/>
  </system-properties>
</appengine-web-app>
